@using MyEvernote.Entities.Messages
@model List<ErrorMessageObj>
@{
    ViewBag.Title = "Geçersiz İşlem";
}

<div class="container">
    <div class="row text-center">
        <div class="col-sm-4 col-sm-offset-4">
            <div class="form-header">
                <h1 class="text-center">Yönlendiriliyorsunuz...</h1>
            </div>
            <div class="main-content">
                <div class="text-danger">
                    <h2><span class="glyphicon glyphicon-remove"></span>&nbsp;&nbsp;&nbsp;Hesap geçerli değil...</h2>
                    <hr />
                    <p class="text-justify">
                        @foreach (ErrorMessageObj err in Model)
                        {
                            @err.Message<br>
                        }
                    </p>
                </div>
            </div>
        </div>
    </div>
</div>

<script>

    $(function () {
        $('footer').parent().hide()
        setTimeout(function () {

            window.location = "/Home/Index";

        }, 5000)
    });

</script>